[
  {
    "ClientId": "aitrainer-local",
    "Description": "Client for implicit flow",
    "AllowedGrantTypes": ["authorization_code", "password"],
    "AllowAccessTokensViaBrowser": true,
    "RedirectUris": [
      "http://localhost:3000/oidc-signin",
      "http://localhost:3000/oidc-silent-renew",
      "http://localhost:3000/oidc-signout",
      "http://localhost:5234/oidc-signin",
      "http://localhost:5234/oidc-silent-renew",
      "http://localhost:5234/oidc-signout",
      "http://localhost:5222/oidc-signin",
      "http://localhost:5222/oidc-silent-renew",
      "http://localhost:5222/oidc-signout",
      "http://localhost:80/oidc-signin",
      "http://localhost:80/oidc-silent-renew",
      "http://localhost:80/oidc-signout"
    ],
    "AllowedScopes": ["openid", "profile", "email"],
    "RequireClientSecret": false,
    "IdentityTokenLifetime": 3600,
    "AccessTokenLifetime": 3600
  },
  {
    "ClientId": "aitrainer-k6-local",
    "Description": "Client for k6 tests",
    "AllowedGrantTypes": ["password"],
    "AllowAccessTokensViaBrowser": true,
    "AllowedScopes": ["openid", "profile", "email"],
    "RequireClientSecret": false,
    "IdentityTokenLifetime": 3600,
    "AccessTokenLifetime": 3600
  }
]
